{
  "name": "api",
  "version": "1.0.0",
  "prisma": {
    "schema": "./src/databases/prisma/schema.prisma",
    "seed": "ts-node ./src/databases/prisma/seed.ts"
  },
  "scripts": {
    "watch": "concurrently \"yarn watch-ts\" \"yarn watch-node\"",
    "watch-ts": "tsc -w",
    "build-ts": "tsc",
    "build": "yarn run eslint && npx prisma generate && yarn run build-ts",
    "watch-node": "nodemon -r dotenv/config build/server.js",
    "eslint": "eslint .eslintrc.json",
    "seed": "npx prisma db seed",
    "eslint:fix": "eslint -c .eslintrc.json src --fix",
    "format": "prettier . --write"
  },
  "devDependencies": {
    "@overnightjs/core": "^1.7.6",
    "@prisma/client": "^5.17.0",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "argon2": "^0.40.3",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "express": "^4.19.2",
    "handy-redis": "^2.3.1",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.4",
    "prettier": "3.3.3",
    "prisma": "^5.17.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0",
    "uuid": "^10.0.0"
  }
}
